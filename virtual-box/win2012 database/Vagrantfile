Vagrant.configure(2) do |config|
    config.vm.guest = :windows
    config.vm.communicator = "winrm"
    config.vm.boot_timeout = 600
    config.vm.graceful_halt_timeout = 600
    config.vm.network :forwarded_port, guest: 3389, host: 33389
    config.vm.network :forwarded_port, guest: 5985, host: 55985, id: "winrm", auto_correct: true

    config.vm.define "guest" do |guest|
        guest.vm.box = "chrism/win2012-r2-x64"
        guest.vm.hostname = "windb1"
        guest.vm.network :private_network, ip: "192.168.33.11"
        guest.vm.provider "virtualbox" do |virtualbox|
            virtualbox.name = "windb1"
            virtualbox.memory = "2048"
            virtualbox.cpus = "2"

        end
    end
end